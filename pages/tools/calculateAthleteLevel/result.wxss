/* pages/tools/calculateAthleteLevel/result.wxss */

.tr {
    width: 100%;
    display: flex;
    justify-content: space-between;
}

.level, .itemName, .td,.yourScore {
    padding: 10px;
    border-bottom: 1px solid #DADADA;
    border-right: 1px solid #DADADA;
    text-align: center;
    width: 100%;
}

.level {
    /*font-weight: bold;*/
    background-color: #DADADA;
}

.itemName {
    font-weight: bold;
    background-color: #9D9D9D;
}

.yourScore {
    font-weight: bold;
    background-color: #CAFFFF;
}
